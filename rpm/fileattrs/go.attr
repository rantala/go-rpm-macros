%__go_path              ^%{gopath}/src/.+/.goipath$
%__go_flags             path
%__go_provides          go-rpm-integration               provides --prefix "%{buildroot}" --go-path "%{gopath}" --version "%{?epoch:%{epoch}:}%{version}-%{release}" %{?commit:-a "(commit=%{commit})"} %{?branch:-a "(branch=%{branch})"} %{?tag:-a "(tag=%{tag})"} %{?goprovflags}
%__go_requires          go-rpm-integration               requires --prefix "%{buildroot}" --go-path "%{gopath}" %{?goreqflags}
