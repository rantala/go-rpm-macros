%__go_provides          %{_rpmconfigdir}/go.prov --prefix "%{buildroot}" --goipps "%{gobaseipath}%{?__go_additional_ipps::%{__go_additional_ipps}}" --version "%{?epoch:%{epoch}:}%{version}-%{release}" --ignore-dirs "%{__go_ignore_dirs}" --ignore-trees "%{__go_ignore_trees}" --ignore-regex "%{__go_ignore_regex}"
%__go_requires          %{_rpmconfigdir}/go.req  --prefix "%{buildroot}" --goipps "%{gobaseipath}%{?__go_additional_ipps::%{__go_additional_ipps}}" --ignore-dirs "%{__go_ignore_dirs}" --ignore-trees "%{__go_ignore_trees}" --ignore-regex "%{__go_ignore_regex}"
%__go_path              ^%{gopath}/src/.+/.*$
